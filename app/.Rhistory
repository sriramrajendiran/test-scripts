library(SPARQL)
endpoint <- "http://http://dbpedia.org/snorql/"
query <-
"PREFIX dbpedia0: <http://dbpedia.org/ontology/>
SELECT ?cat
WHERE {
<http://dbpedia.org/resource/Suriya> dbpedia0:abstract ?cat .
FILTER (LANG(?cat)='en')
}"
qd <- SPARQL(endpoint,query)
df <- qd$results
